# CSV Processor Agent Environment Variables
# Copy this file to .env and customize as needed

# Model configuration
CSV_PROCESSOR_MODEL=gemini-2.5-flash

# Processing limits
MAX_CSV_SIZE_MB=50
MAX_ROWS_PREVIEW=5
MAX_CATEGORICAL_VALUES=10

# Feature flags
ENABLE_ADVANCED_STATS=true
ENABLE_DATA_PROFILING=true
